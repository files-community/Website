{
	"name": "files-community",
	"version": "0.0.1",
	"repository": "https://github.com/files-community/files",
	"scripts": {
		"dev": "vite dev --host",
		"build": "vite build",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --ignore-unknown --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --write --ignore-unknown --plugin-search-dir=. .",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@fec/remark-a11y-emoji": "^3.1.0",
		"@iconify-json/fluent": "^1.1.24",
		"@neodrag/svelte": "^2.0.3",
		"@sveltejs/adapter-cloudflare": "^2.3.0",
		"@sveltejs/kit": "^1.20.5",
		"@typescript-eslint/eslint-plugin": "^5.59.11",
		"@typescript-eslint/parser": "^5.60.0",
		"autoprefixer": "^10.4.14",
		"cssnano": "^6.0.1",
		"eslint": "^8.43.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte": "^2.31.0",
		"fluent-svelte": "^1.6.0",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.2",
		"mdsvex": "^0.10.6",
		"postcss": "^8.4.24",
		"postcss-media-minmax": "^5.0.0",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"remark-gfm": "^3.0.1",
		"remark-github": "^11.2.4",
		"remark-slug": "^7.0.1",
		"sass": "^1.63.4",
		"svelte": "^3.59.2",
		"svelte-check": "^3.4.4",
		"svelte-i18n": "^3.6.0",
		"svelte-preprocess": "^5.0.4",
		"tslib": "^2.5.3",
		"typescript": "^5.1.3",
		"unplugin-icons": "0.16.3",
		"vite": "^4.3.9"
	},
	"lint-staged": {
		"*": "prettier --write --ignore-unknown --plugin-search-dir=."
	},
	"type": "module"
}
